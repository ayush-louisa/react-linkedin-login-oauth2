{
  "name": "react-linkedin-login-oauth2",
  "version": "2.0.1",
  "description": "React component for Linked In Log In feature using OAuth 2.0",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "assets"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean && npm run lint && npm run type-check",
    "dev": "vite ./preview",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit",
    "prettier": "prettier src --write",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "publish:local": "npm publish --registry http://localhost:4873",
    "publish:alpha": "npm publish --tag alpha",
    "publish:latest": "npm publish"
  },
  "peerDependencies": {
    "react": "17.x || 18.x || 19.x",
    "react-dom": "17.x || 18.x || 19.x"
  },
  "devDependencies": {
    "@testing-library/react": "^16.1.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.5",
    "@eslint/js": "^9.17.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jsdom": "^25.0.1",
    "prettier": "^3.4.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.2",
    "vite": "^6.1.7",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^3.2.4",
    "@vitest/ui": "^3.2.4"
  },
  "author": "nvh95",
  "homepage": "https://github.com/nvh95/react-linkedin-login-oauth2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nvh95/react-linkedin-login-oauth2.git"
  },
  "bug": {
    "url": "https://github.com/nvh95/react-linkedin-login-oauth2/issues"
  },
  "keywords": [
    "react",
    "linkedin",
    "login",
    "signin",
    "oauth",
    "oauth2",
    "redirect"
  ]
}
